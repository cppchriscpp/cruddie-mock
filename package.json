{
  "name": "cruddie-mock",
  "version": "0.0.1",
  "description": "Node module for quickly spinning up CRUD APIs with mock data based on json model schemas.",
  "main": "./src/api.js",
  "scripts": {
    "test": "node_modules/.bin/istanbul cover node_modules/jasmine/bin/jasmine.js --include-all-sources -x **/*_spec.js -x bin/**/* -x public/**/* -x scripts/**/*",
    "print_faker_docs": "node ./scripts/generate_docs.js"
  },
  "bin": {
    "cruddie-mock": "bin/cruddie-mock.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cppchriscpp/cruddie-mock"
  },
  "preferGlobal": true,
  "keywords": [
    "rest",
    "json",
    "mock",
    "api",
    "crud",
    "schema",
    "simple",
    "swagger"
  ],
  "author": "cppchriscpp",
  "license": "MIT",
  "dependencies": {
    "express": "^4.15.4",
    "faker": "^4.1.0",
    "json-server": "^0.12.0",
    "minimist": "^1.2.0",
    "swagger-ui-dist": "^3.1.7"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "jasmine": "^2.8.0",
    "proxyquire": "^1.8.0",
    "request": "^2.81.0"
  }
}
